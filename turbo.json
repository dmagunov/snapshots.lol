{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".next/**",
        "artifacts/**",
        "deployments/**",
        "cache/**",
        "typechain/**"
      ]
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false
    }
  },
  "globalEnv": [
    "NEXT_PUBLIC_URL",
    "NEXT_PUBLIC_DEFAULT_CHAIN",
    "NEXT_PUBLIC_SUPPORTED_CHAINS",
    "NEXT_PUBLIC_RPC_31337",
    "NEXT_PUBLIC_RPC_1",
    "NEXT_PUBLIC_RPC_5"
  ]
}
